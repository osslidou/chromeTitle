{
    "manifest_version": 2,
    "name": "Chrome Title",
    "options_page": "popup.html",
    "version": "1.0",
    "background": {
        "scripts": [
            "jquery-2.1.4.min.js",     
            "jquery-3.1.1.min.js",     
            "background.js"
        ]
    },
    "browser_action": {
         "name": "Click to change the icon's color" 
    },
    "permissions": [
        "tabs",
        "<all_urls>",
        "http://*/*",
        "https://*/*",
        "contextMenus",
        "topSites","notifications","storage"
    ],   
    /*"icons": {
          "16": "icon-bitty.png"
        },*/
    "content_security_policy": "script-src 'self' https://ajax.googleapis.com; object-src 'self'"
}